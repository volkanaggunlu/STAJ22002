/*==========================================================================
* Google Fonts
==========================================================================*/
@import url("https://fonts.googleapis.com/css2?family=Space+Grotesk:wght@400;500;700&display=swap");
/*==========================================================================
* Common CSS
==========================================================================*/
body {
  font-family: var(--body-font);
  color: var(--body-color);
  font-size: 16px;
  line-height: 26px;
  font-weight: 400;
  text-transform: capitalize;
}

img {
  max-width: 100%;
  height: auto;
  transition: 0.4s;
}

a {
  outline: none;
  color: inherit;
  text-decoration: none;
}

a,
button,
i {
  text-decoration: none;
  color: inherit;
}

a:focus,
a:hover {
  text-decoration: none;
  color: inherit;
}

.section-padding {
  padding: 120px 0px;
}

@media (max-width: 991px) {
